[realms]
kRb5_LiBdEf_DeFaUlT_ReAlM = {
  kdc = kRb5_rEaLmS_dEf_DoM:88
  admin_server = kRb5_rEaLmS_dEf_DoM:749
  default_domain= kRb5_rEaLmS_dEf_DoM 
 }

[domain_realm]
 .kRb5_DoMaIn_ReAlM = kRb5_LiBdEf_DeFaUlT_ReAlM 
